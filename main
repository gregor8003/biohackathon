import kivy
from kivy.properties import NumericProperty

kivy.require('1.0.6') # replace with your current kivy version !

from kivy.app import App
from kivy.uix.gridlayout import GridLayout


class Archiver(App):
    label_1 = 'SSSSSSSSSS'

    def build(self):

        return GridLayout()

if __name__ == '__main__':
    Archiver().run()
